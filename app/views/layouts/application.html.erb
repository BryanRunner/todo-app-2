<!DOCTYPE html>
<html>
<head>
  <title>TodoApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
  <div class="row">
    <div class="col-md-4">
      <div id="todo-wrap">
        <div class="index-wrap">
          <%= yield %>
        </div>
        <div class="content-wrap">
          <div class="content"></div>
        </div>
      </div>
    </div>
  </div>
</div>

</body>
</html>
